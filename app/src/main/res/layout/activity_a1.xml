<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".A1"
    tools:layout_editor_absoluteY="25dp">


    <TextView
        android:id="@+id/textView"
        style="@style/Banner"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.153"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/pTatiana"
        style="@style/boton"
        android:onClick="onClickTatiana"
        app:layout_constraintBottom_toTopOf="@+id/pMikhail"
        app:layout_constraintEnd_toStartOf="@+id/textView3"
        app:layout_constraintHorizontal_bias="0.035"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:srcCompat="@drawable/tatiana" />

    <ImageButton
        style="@style/boton"
        android:id="@+id/pMikhail"
        android:onClick="onClickMikha"
        app:layout_constraintBottom_toTopOf="@+id/pMabel"
        app:layout_constraintEnd_toStartOf="@+id/textView4"
        app:layout_constraintHorizontal_bias="0.133"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pTatiana"
        app:srcCompat="@drawable/mikha" />

    <ImageButton
        style="@style/boton"
        android:id="@+id/pMabel"
        android:onClick="onClickMabel"
        app:layout_constraintBottom_toTopOf="@+id/pIsaac"
        app:layout_constraintEnd_toStartOf="@+id/textView5"
        app:layout_constraintHorizontal_bias="0.033"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pMikhail"
        app:srcCompat="@drawable/mabel" />

    <ImageButton
        style="@style/boton"
        android:id="@+id/pIsaac"
        android:onClick="onClickIsaac"
        app:layout_constraintBottom_toTopOf="@+id/pDani"
        app:layout_constraintEnd_toStartOf="@+id/textView6"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pMabel"
        app:srcCompat="@drawable/isaac" />

    <ImageButton
        style="@style/boton"
        android:id="@+id/pDani"
        android:onClick="onClickDaniel"
        app:layout_constraintBottom_toTopOf="@+id/pXavi"
        app:layout_constraintEnd_toStartOf="@+id/textView2"
        app:layout_constraintHorizontal_bias="0.111"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pIsaac"
        app:srcCompat="@drawable/dani" />

    <ImageButton
        style="@style/boton"
        android:id="@+id/pXavi"
        android:onClick="onClickXavi"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView7"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pDani"
        app:srcCompat="@drawable/xavi" />

    <TextView
        style="@style/Texto1"
        android:id="@+id/textView2"
        android:text="@string/p5"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        style="@style/Texto1"
        android:id="@+id/textView7"
        android:text="@string/p6"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView6"
        style="@style/Texto1"
        android:layout_width="231dp"
        android:text="@string/p4"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        style="@style/Texto1"
        android:id="@+id/textView5"
        android:text="@string/p3"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        style="@style/Texto1"
        android:id="@+id/textView4"
        android:text="@string/p2"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        style="@style/Texto1"
        android:id="@+id/textView3"
        android:text="@string/p1"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

</android.support.constraint.ConstraintLayout>